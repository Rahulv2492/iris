        "stylelint-processor-styled-components": "^1.10.0",
        "ts-loader": "^6.2.1",
        "typescript": "3.8.2",
        "typescript-plugin-styled-components": "^1.4.4",
        "webpack": "^4.41.6"
    },
    "peerDependencies": {
        "polished": "^3.4.2",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "styled-components": "^5.0.1"
    },
    "resolutions": {
        "@types/react": "*"
    },
    "scripts": {
        "build-styleguide": "build-storybook -c .storybook -o build-styleguide",
        "build": "npm run clean && npx tsc",
        "checkup": "npx npm-check-updates",
        "clean": "rm -rf build",
        "dev": "npm run storybook",
        "lint-pretty": "eslint 'src/**/*.{ts,tsx}' -f codeFrame && npm run prettier && npm run stylelint",
        "lint": "eslint 'src/**/*.{ts,tsx}' && npm run prettier && npm run stylelint",
        "postpublish": "git clean -fd",
        "prettier": "prettier --list-different 'src/**/*.{ts,tsx}'",
        "prepublishOnly": "npm run typecheck && npm run lint && npm run build && cp -r ./build/* ./ && npm run clean",
        "start": "npm run typecheck && npm run lint && npm run test && npm run storybook",
        "storybook": "start-storybook -p 9001 -c .storybook --ci",
        "deploy-storybook": "deploy-storybook -a iris",
        "stylelint": "npx stylelint 'src/**/*.ts'",
        "test-publish": "npm run prepublishOnly && npm run postpublish",
        "test": "jest",
        "typecheck": "npx tsc --noEmit"
    },
    "jest": {
        "setupFiles": [
            "./.jest/register-context.js"
        ],
        "transform": {
            "^.+\\.[t|j]sx?$": "babel-jest",
            "^.+\\.mdx?$": "@storybook/addon-docs/jest-transform-mdx"
        },
        "transformIgnorePatterns": [
            "/node_modules/(?!@nox/addon-themes|react-syntax-highlighter).+\\.js$"
        ]
    },
    "pre-commit": [
        "lint-pretty"
    ],
    "publishConfig": {
        "registry": "https://artifactory.vimeows.com/artifactory/api/npm/npm/"
    },
    "bugs": {
        "url": "https://github.vimeows.com/Vimeo/iris/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.vimeows.com/Vimeo/iris.git"
    },
    "engines": {
        "node": ">=8.0.0",
        "yarn": ">=1.6.0"
    },
    "keywords": [
        "Vimeo",
        "UI Library",
        "Component Library",
        "Design System",
        "TypeScript",
        "JavaScript",
        "React",
        "styled-components"
    ]
}
