        "typescript-plugin-styled-components": "^1.2.0"
    },
    "dependencies": {
        "@types/clipboard": "^2.0.1",
        "@types/lodash": "^4.14.120",
        "@types/react": "^16.8.2",
        "@types/react-dom": "^16.8.0",
        "@types/styled-components": "^3.0.2",
        "clipboard": "^2.0.4",
        "create-react-ref": "^0.1.0",
        "lodash.throttle": "^4.1.1",
        "moment": "^2.24.0",
        "polished": "^2.3.3",
        "react": "^16.8.0",
        "react-color": "^2.17.0",
        "react-datetime": "^2.16.3",
        "react-dom": "next",
        "react-popper": "^0.10.4",
        "react-swipe": "^6.0.4",
        "react-swipeable": "^4.3.1",
        "react-tether": "1.0.2",
        "react-transition-group": "^2.5.3",
        "styled-components": "^3.5.0-0"
    },
    "peerDependencies": {
        "@types/react": "^16.8.2",
        "@types/react-dom": "^16.8.0",
        "polished": "^2.3.3",
        "styled-components": "^3.5.0-0"
    },
    "scripts": {
        "dev": "npx tsc -w",
        "clean": "rm -rf dist; find . -maxdepth 1 -type d -name \"dist-*\" -exec rm -rf \"{}\" \\;",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "svg": "npm run svg-min; npm run svg-react",
        "svg-min": "node scripts/svg-index; npx prettier ./src/Icons/index.ts --write; npx prettier ./src/Illustrations/index.ts --write",
        "svg-react": "npm run svg-react-icons; npm run svg-react-illustrations;",
        "svg-react-icons": "npx @svgr/cli --template ./scripts/svg-react.js --ext tsx -d ./src/Icons ./src/Icons; rm -f ./src/Icons/*.svg",
        "svg-react-illustrations": "npx @svgr/cli --template ./scripts/svg-react.js --ext tsx -d ./src/Illustrations ./src/Illustrations; rm -f ./src/Illustrations/*.svg",
        "build": "npm run clean; npm run svg; npx tsc",
        "build-styleguide": "build-storybook -c .storybook -o build-styleguide",
        "test-publish": "npm run prepublishOnly; npm run postpublish;",
        "prepublishOnly": "npm run lint; npm run build; node scripts/uid-dist; cp -r ./modules/* ./",
        "postpublish": "npm run clean; rm color.d.ts color.js components.d.ts components.js icons.d.ts icons.js illustration.d.ts illustration.js index.d.ts index.js layout.d.ts layout.js typography.d.ts typography.js",
        "lint": "tslint 'src/**/*.{ts,tsx}' -e '**/*.story.tsx'; prettier --list-different 'src/**/*.{ts,tsx}'",
        "lint-pretty": "tslint 'src/**/*.{ts,tsx}' -e '**/*.story.tsx' -t codeFrame; prettier --list-different 'src/**/*.{ts,tsx}'"
    },
    "pre-commit": [
        "lint-pretty"
    ],
    "publishConfig": {
        "registry": "https://artifactory.vimeows.com/artifactory/api/npm/npm/"
    },
    "bugs": {
        "url": "https://github.vimeows.com/Vimeo/iris/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.vimeows.com/Vimeo/iris.git"
    },
    "engines": {
        "node": ">=8.0.0",
        "yarn": ">=1.6.0"
    },
    "keywords": [
        "Vimeo",
        "UI Library",
        "Component Library",
        "Design System",
        "TypeScript",
        "JavaScript",
        "React",
        "styled-components"
    ]
}