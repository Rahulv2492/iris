        "eslint": "^5.13.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "eslint-plugin-react-hooks": "^1.0.0",
        "fork-ts-checker-webpack-plugin": "^0.5.2",
        "pascal-case": "^2.0.1",
        "pre-commit": "^1.2.2",
        "prettier": "^1.16.4",
