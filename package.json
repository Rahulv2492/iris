        "Vimeo",
        "UI Library",
        "Component Library",
        "Design System",
        "TypeScript",
        "JavaScript",
        "React",
        "styled-components"
    ],
    "rv": {
        "mode": "library",
        "dist": "./build",
        "storybook": {
            "configDir": ".storybook",
            "ci": true
        }
    }
