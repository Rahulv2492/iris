    "description": "Iris UI Pattern Framework",
    "contributors": [
        {
            "name": "Sean McIntyre",
            "email": "sean.mcintyre@vimeo.com"
        }
    ],
    "main": "index.js",
    "types": "index.d.ts",
    "sideEffects": false,
    "devDependencies": {
        "@babel/core": "^7.8.6",
        "@babel/plugin-proposal-export-default-from": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.8.3",
